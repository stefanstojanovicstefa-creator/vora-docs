{
  "name": "vora-backend",
  "version": "0.1.0",
  "description": "Vora AI Voice Platform - Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc || echo 'TypeScript compilation had warnings, but continuing...'",
    "postbuild": "prisma generate",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "lint": "eslint src --ext .ts",
    "lint:ci": "eslint src --ext .ts --max-warnings 0",
    "type-check": "tsc --noEmit",
    "test": "jest --runInBand",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:security": "jest --testPathPattern=security",
    "test:security:coverage": "jest --testPathPattern=security --coverage --collectCoverageFrom='src/**/*.ts' --coverageDirectory=coverage/security",
    "test:security:watch": "jest --testPathPattern=security --watch",
    "test:ai-generator": "tsx tests/ai-test-generator.ts",
    "test:comprehensive": "tsx tests/ai-comprehensive-tester.ts",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:security && npm run test:e2e",
    "debug:dashboard": "tsx src/debug/dashboard-server.ts",
    "deploy:staging": "bash scripts/deploy-staging.sh",
    "deploy:production": "bash scripts/deploy-production.sh",
    "rollback:staging": "bash scripts/rollback.sh staging",
    "rollback:production": "bash scripts/rollback.sh production",
    "smoke:staging": "bash scripts/smoke-test.sh staging",
    "smoke:production": "bash scripts/smoke-test.sh production",
    "smoke:crm": "tsx scripts/smoke-crm.ts",
    "seed:local": "tsx prisma/seed-local.ts",
    "seed:staging": "tsx prisma/seed-staging.ts",
    "seed:interview-conductor": "tsx prisma/seed-interview-conductor.ts",
    "seed:forge-conductor": "tsx prisma/seed-forge-conductor.ts",
    "setup:staging": "bash scripts/setup-staging.sh",
    "prepare": "husky",
    "db:rollback": "tsx scripts/rollback-migration.ts",
    "db:rollback:dry": "tsx scripts/rollback-migration.ts --dry-run",
    "db:rollback:force": "tsx scripts/rollback-migration.ts --force --backup",
    "load:api": "k6 run tests/load/api-load-test.js",
    "load:stress": "k6 run tests/load/stress-test.js",
    "load:soak": "k6 run tests/load/soak-test.js",
    "load:all": "npm run load:api && npm run load:stress",
    "load:quick": "node scripts/load/run-k6.mjs tests/load/api-load-test.js --env SMOKE=true --env VUS=2 --env DURATION=1m",
    "load:api:dotenv": "node scripts/load/run-k6.mjs tests/load/api-load-test.js",
    "load:stress:dotenv": "node scripts/load/run-k6.mjs tests/load/stress-test.js",
    "load:soak:dotenv": "node scripts/load/run-k6.mjs tests/load/soak-test.js",
    "load:voice:dotenv": "node scripts/load/run-k6.mjs tests/load/voice-sessions.js",
    "load:concurrent:dotenv": "node scripts/load/run-k6.mjs tests/load/concurrent-sessions.js",
    "deploy:interview-conductor": "tsx scripts/deploy-interview-conductor.ts",
    "benchmark:latency": "tsx scripts/benchmarks/latency-benchmark.ts",
    "benchmark:latency:global": "tsx scripts/benchmarks/latency-benchmark.ts -- --tier=global --iterations=100",
    "benchmark:latency:standard": "tsx scripts/benchmarks/latency-benchmark.ts -- --tier=standard --iterations=100",
    "benchmark:latency:premium": "tsx scripts/benchmarks/latency-benchmark.ts -- --tier=premium --iterations=100",
    "benchmark:latency:all": "npm run benchmark:latency:global && npm run benchmark:latency:standard && npm run benchmark:latency:premium"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.9",
    "@clerk/clerk-sdk-node": "^4.13.23",
    "@deepgram/sdk": "^4.11.3",
    "@google-cloud/speech": "^7.2.1",
    "@google-cloud/text-to-speech": "^6.4.0",
    "@google/generative-ai": "^0.21.0",
    "@hubspot/api-client": "^13.4.0",
    "@mistralai/mistralai": "^1.11.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^5.22.0",
    "@sentry/node": "^10.31.0",
    "@sentry/profiling-node": "^10.31.0",
    "@supabase/supabase-js": "^2.90.1",
    "@types/archiver": "^7.0.0",
    "@upstash/redis": "^1.36.0",
    "@xenova/transformers": "^2.17.2",
    "ai21": "^1.3.0",
    "archiver": "^7.0.1",
    "assemblyai": "^4.22.1",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.6.2",
    "bullmq": "^5.66.2",
    "chrono-node": "^2.9.0",
    "cohere-ai": "^7.20.0",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "decimal.js": "^10.6.0",
    "dotenv": "^16.4.5",
    "elevenlabs": "^1.59.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.1",
    "groq-sdk": "^0.37.0",
    "ioredis": "^5.8.2",
    "isolated-vm": "^6.0.2",
    "jsforce": "^2.0.0-beta.29",
    "jsonrepair": "^3.13.2",
    "jsonwebtoken": "^9.0.3",
    "livekit-server-sdk": "^2.7.1",
    "lru-cache": "^10.4.3",
    "mammoth": "^1.11.0",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "openai": "^6.15.0",
    "opossum": "^9.0.0",
    "pdf-parse": "^1.1.4",
    "prisma": "^5.22.0",
    "prom-client": "^15.1.3",
    "puppeteer": "^24.32.1",
    "socket.io": "^4.8.1",
    "svix": "^1.82.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tiktoken": "^1.0.22",
    "typescript": "^5.6.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/bcrypt": "^6.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.9.1",
    "@types/node-cron": "^3.0.11",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "chalk": "^4.1.2",
    "eslint": "^9.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0",
    "lint-staged": "^15.5.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.19.2",
    "ws": "^8.16.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
